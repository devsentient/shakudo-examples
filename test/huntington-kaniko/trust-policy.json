{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Federated": "arn:aws:iam::<Account ID>:oidc-provider/oidc.eks.us-west-2.amazonaws.com/id/<oidc id>"
      },
      "Action": "sts:AssumeRoleWithWebIdentity",
      "Condition": {
        "StringEquals": {
          "oidc.eks.us-west-2.amazonaws.com/id/<oidc id>:sub": "system:serviceaccount:hyperplane-pipelines:gcr-pipelines"
        }
      }
    }
  ]
}